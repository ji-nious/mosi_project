CREATE TABLE SELLER_PAGE (
    PAGE_ID       NUMBER(10) PRIMARY KEY,
    MEMBER_ID     NUMBER(10) NOT NULL,
    IMAGE         BLOB,
    INTRO         VARCHAR2(500),
    SALES_COUNT   NUMBER(10, 0) DEFAULT 0,
    REVIEW_AVG    NUMBER(3, 2) DEFAULT 0,
    CREATE_DATE   TIMESTAMP DEFAULT SYSTIMESTAMP,
    UPDATE_DATE   TIMESTAMP DEFAULT SYSTIMESTAMP,
    CONSTRAINT FK_MEMBER FOREIGN KEY (MEMBER_ID)
        REFERENCES MEMBER(MEMBER_ID)
);

CREATE SEQUENCE SEQ_SELLER_PAGE START WITH 1 INCREMENT BY 1;
